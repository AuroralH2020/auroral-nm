{
  "name": "nodejs-ts-starter",
  "version": "1.0.0",
  "description": "Base for future ts projects",
  "main": "server.js",
  "scripts": {
    "start": "node ./dist/src/server.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "start:watch": "nodemon ./dist/src/server.js",
    "dev": "concurrently \"npm run build:watch\" \"npm run start:watch\"",
    "prod": "concurrently \"npm run build\" \"npm run start\""
  },
  "author": "Jorge Almela",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/got": "^9.6.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/stoppable": "^1.1.0",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "got": "^11.8.1",
    "jsonwebtoken": "^8.5.1",
    "qs": "^6.9.6",
    "stoppable": "^1.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  }
}
